[
  {
    "server": "nats",
    "config": {
      "name": "generic",
      "subjects": [
        "events.*"
      ],
      "retention": "limits",
      "max_consumers": -1,
      "max_msgs_per_subject": -1,
      "max_msgs": 20000,
      "max_bytes": 10485760,
      "max_age": 1800000000000,
      "max_msg_size": 2097152,
      "storage": "memory",
      "discard": "old",
      "num_replicas": 1,
      "duplicate_window": 12000000000,
      "sealed": false,
      "deny_delete": false,
      "deny_purge": false,
      "allow_rollup_hdrs": false,
      "description": "Generic in-memory stream for events and orders"
    }
  }
]
